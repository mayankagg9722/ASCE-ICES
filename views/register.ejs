  <script>
      $(window).load(function(){
      $(".preloader").fadeOut(500);
      }); 
      function registerSubmit(){
        $("#reg").attr('disabled',true);
        $(".preloader").fadeIn(200);
        $("#msg").text(""); 
        // console.log("hello I am here");
        $.ajax({
        url: "/signup",
        data:
        {
          "firstName":$("#fname").val(),
          "lastName":$("#lname").val(),
          "college":$("#college").val(),
          "email":$("#email").val(),
          "mobileNumber":$("#mobileNumber").val(),
          "password":$("#pass").val(),
          "confirmPassword":$("#confirm").val()
        },
        type: "POST",
        success: function (json) {
          $(".preloader").fadeOut(500);
          // console.log(json);
            // if(json.success==false)
            // {
            $("#msg").text(json.message);   
        },
        error: function (msg) {
          // console.log("error");
        }
      });
      }
    $(document).ready(function(){
      // $(".form-content").css("height",$(window).height()-$("footer").outerHeight()-$(".altr").height()+'px');
        if($(window).width()>=992)
        {
            $(".item-price,.item-img").css("height",$(".cart-item").height()+'px');
            $(".altr").css("height",$(".pc-header").height()+'px');
        }
        else
        {
         $(".altr").css("height",$(".mobile-header").height()+'px');
      }

    });
    </script>
