<div class="container-fluid row">

    <form id="signupForm" class=" pos col-xs-10 col-xs-offset-1 col-sm-6 col-sm-offset-3 col-lg-4 col-lg-offset-4">

        <h2 class="ui block header">Register</h2>

        <div class="ui fluid icon input custom">
          <img class="icons" src="/images/user.png">
          <input type="text" placeholder="First Name" id="fname" name="firstName">
        </div>

        <div class="ui fluid icon input custom">
          <img class="icons" src="/images/user.png">
          <input type="text" placeholder="Last Name" id="lname" name="lastName">
        </div>

        <div class="ui fluid icon input custom">
          <img class="icons" src="/images/college.png">
          <input type="text" placeholder="College" id="college" name="college">
        </div>

        <div class="ui fluid icon input custom">
          <img class="icons" src="/images/phone.png">
          <input type="text" placeholder="Mobile Number" id="mobileNumber" name="mobileNumber">
        </div>

        <div class="ui fluid icon input custom">
          <img class="icons" src="/images/mail.png">
          <input type="email" placeholder="Email" id="email" name="email">
        </div>

        <div class="ui fluid action input custom">
            <img  class="icons" src="/images/lock.png">
            <input type="password" placeholder="Password" id="pass" name="password">
        </div>

        <div class="ui fluid action input custom">
            <img  class="icons" src="/images/lock.png">
            <input type="password" placeholder="Retype Password" id="confirm" name="confirmPassword">
        </div>



        <input class="fluid ui button" value="Submit" type="button" id="reg" onclick="registerSubmit()">
        <br>
        <p id="msg"></p>
      </form>


</div>
