    <!-- <script src="../../assets/js/ie10-viewport-bug-workaround.js"></script> -->
    <script>
    function loginSubmit(){
      $(".preloader").fadeIn(200);
        $.ajax({
        url: "/login",
        data:
        {
          "contingentID":$("#id").val(),
          "password":$("#pass").val()
        },
        type: "POST",
        success: function (json) {
          $(".preloader").fadeOut(500);
            if(json.success==false)
            {
              $("#msg").text(json.message);
            }
            else if(json.code==201)
            {
              window.location.href="/admin"
            }
            else if(json.code==200)
            {
              window.location.href="/profile"
            }
            // else
            // {
            //   if($("#contingentID").val())
            //   window.location.href="/home"
            // }    
        },
        error: function (json) {
        }
      });
      }
      $(window).load(function(){
      $(".preloader").fadeOut(500);
      }); 
    $(document).ready(function(){
      $("#pass,#id").on("keydown",function(evt){
        if(evt.which==13)
        {
          loginSubmit();
        }
      });
      // $(".form-content").css("height",$(window).height()-$("footer").outerHeight()-$(".altr").height()+'px');
        if($(window).width()>=992)
        {
            $(".item-price,.item-img").css("height",$(".cart-item").height()+'px');
            $(".altr").css("height",$(".pc-header").height()+'px');
            $("footer").css({position:"absolute",bottom:"0px"});
        }
        else
        {
         $(".altr").css("height",$(".mobile-header").height()+'px');
      }
      $(".body").css("height",$(window).height()+'px');

    });
  </script>
