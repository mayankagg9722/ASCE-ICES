        <h2 class="ui block header">Login</h2>

        <div class="ui fluid icon input">
          <img class="icons" src="/images/user.png">
          <input type="text" placeholder="Contingent ID" id="id"  name="contingentID">
        </div>

        <div class="ui fluid action input">
            <img  class="icons" src="/images/lock.png">
            <input type="password" id="pass" placeholder="Password"  name="password">
        </div>

        <input class="fluid ui button" id="login" value="Submit" type="button" onclick="loginSubmit()"/>
        <h2 class="ui block header customField">New to us? &nbsp;&nbsp;<a href="/register">Register</a></h2>

