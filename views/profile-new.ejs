var scrollDuration = 400;
var leftPaddleEvent = document.getElementsByClassName('left-paddle-event');
var rightPaddleEvent = document.getElementsByClassName('right-paddle-event');
var leftPaddleWorkshop = document.getElementsByClassName('left-paddle-workshop');
var rightPaddleWorkshop = document.getElementsByClassName('right-paddle-workshop');
var leftPaddleAccomod = document.getElementsByClassName('left-paddle-accomod');
var rightPaddleAccomod = document.getElementsByClassName('right-paddle-accomod');
var itemsLength = $('.item').length;
var itemSize = $('.item').outerWidth(true);
var paddleMargin = 20;
var getMenuWrapperSize = function() {
	return $('.menu-wrapper').outerWidth();
}
var eventPos=0;
workshopPos=0;
accomodPos=0;
var menuWrapperSize = getMenuWrapperSize();
$(window).on('resize', function() {
	menuWrapperSize = getMenuWrapperSize();
});
var menuVisibleSize = menuWrapperSize;
var getMenuSize = function() {
	return itemsLength * itemSize;
};
menuSize = getMenuSize();
var menuInvisibleSize = menuSize - menuWrapperSize;

var getMenuPositionEvent = function() {
	return $('.menu-event').scrollLeft();
};
var getMenuPositionWorkshop = function() {
	return $('.menu-workshop').scrollLeft();
};
var getMenuPositionAccomod = function() {
	return $('.menu-accomod').scrollLeft();
};
$('.menu-event').on('scroll', function() {

	menuInvisibleSize = menuSize - menuWrapperSize;
	menuPositionEvent = getMenuPositionEvent();
	var menuEndOffset = menuInvisibleSize;
	if (menuPositionEvent <= paddleMargin) {
		$(leftPaddleEvent).addClass('hidden');
		$(rightPaddleEvent).removeClass('hidden');
	} else if (menuPositionEvent < menuEndOffset) {
		// show both paddles in the middle
		$(leftPaddleEvent).removeClass('hidden');
		$(rightPaddleEvent).removeClass('hidden');
	} else if (menuPositionEvent >= menuEndOffset) {
		$(leftPaddleEvent).removeClass('hidden');
		$(rightPaddleEvent).addClass('hidden');
}
});
$('.menu-workshop').on('scroll', function() {

	menuInvisibleSize = menuSize - menuWrapperSize;
	menuPositionWorkshop = getMenuPositionWorkshop();
	var menuEndOffset = menuInvisibleSize;
	if (getMenuPositionWorkshop <= paddleMargin) {
		$(leftPaddleWorkshop).addClass('hidden');
		$(rightPaddleWorkshop).removeClass('hidden');
	} else if (menuPositionWorkshop < menuEndOffset) {
		// show both paddles in the middle
		$(leftPaddleWorkshop).removeClass('hidden');
		$(rightPaddleWorkshop).removeClass('hidden');
	} else if (menuPositionEvent >= menuEndOffset) {
		$(leftPaddleWorkshop).removeClass('hidden');
		$(rightPaddleWorkshop).addClass('hidden');
}
});
$('.menu-accomod').on('scroll', function() {

	menuInvisibleSize = menuSize - menuWrapperSize;
	menuPositionAccomod = getMenuPositionAccomod();
	var menuEndOffset = menuInvisibleSize;
	if (menuPositionAccomod <= paddleMargin) {
		$(leftPaddleAccomod).addClass('hidden');
		$(rightPaddleAccomod).removeClass('hidden');
	} else if (menuPositionAccomod < menuEndOffset) {
		// show both paddles in the middle
		$(leftPaddleAccomod).removeClass('hidden');
		$(rightPaddleAccomod).removeClass('hidden');
	} else if (menuPositionAccomod >= menuEndOffset) {
		$(leftPaddleAccomod).removeClass('hidden');
		$(rightPaddleAccomod).addClass('hidden');
}
});
$(rightPaddleEvent).on('click', function() {
	if($(".menu-event").scrollLeft()+$(".menu-event").outerWidth()<menuSize)
	{
	$('.menu-event').animate( { scrollLeft:$(".menu-event").scrollLeft()+2*$(".item").outerWidth()}, scrollDuration);
}
});
$(leftPaddleEvent).on('click', function() {
	if($(".menu-event").scrollLeft()!==0)
	{
		$('.menu-event').animate( { scrollLeft: $(".menu-event").scrollLeft()-2*$(".item").outerWidth() }, scrollDuration);
	}
});
$(rightPaddleWorkshop).on('click', function() {
	if($(".menu-workshop").scrollLeft()+$(".menu-workshop").outerWidth()<menuSize)
	{
	$('.menu-workshop').animate( { scrollLeft:$(".menu-workshop").scrollLeft()+$(".menu-workshop").outerWidth()}, scrollDuration);
	workshoppos=workshopPos+$(".menu-workshop").outerWidth();
}
});
